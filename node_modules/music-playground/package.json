{
  "name": "music-playground",
  "version": "2.3.0",
  "description": "A Web Audio API library for generating atmospheric soundscapes programmatically. Perfect for adding ambient audio to themed web projects, games, or interactive experiences.",
  "keywords": [
    "web-audio",
    "ambient",
    "soundscape",
    "procedural-audio",
    "themes",
    "audio-synthesis",
    "interactive-audio"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mathonsunday/music-playground.git"
  },
  "type": "module",
  "main": "dist/music-playground.umd.js",
  "module": "dist/music-playground.esm.js",
  "unpkg": "dist/music-playground.umd.js",
  "jsdelivr": "dist/music-playground.umd.js",
  "exports": {
    ".": {
      "import": "./dist/music-playground.esm.js",
      "require": "./dist/music-playground.umd.js",
      "default": "./dist/music-playground.umd.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "typescript": "~5.9.3",
    "vite": "^5.4.0"
  },
  "dependencies": {
    "soundfont-player": "^0.12.0",
    "tone": "^15.1.22"
  }
}
