{
  "name": "narrative-os",
  "version": "1.0.0",
  "description": "An experimental operating system interface designed for deep-sea marine biologist Dr. Mira Petrovic. This project explores innovative user experiences for operating systems through a narrative-driven, character-specific interface.",
  "main": "index.js",
  "scripts": {
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "dead-code": "knip",
    "dead-code:fix": "knip --fix",
    "dead-code:analyze": "dead-code-toolkit analyze",
    "type:coverage": "type-coverage --detail --strict",
    "type:report": "typescript-coverage-report",
    "verify": "npm run type-check && npm run lint && npm run dead-code && npm test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@playwright/test": "^1.57.0",
    "@vitest/ui": "^4.0.17",
    "happy-dom": "^20.1.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "knip": "^5.82.1",
    "lint-staged": "^16.2.7",
    "mock-socket": "^9.3.1",
    "pixelmatch": "^7.1.0",
    "playwright": "^1.57.0",
    "type-coverage": "^2.29.7",
    "typescript-coverage-report": "^1.1.1",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "music-playground": "^2.3.0",
    "visual-toolkit": "^2.2.0"
  }
}
